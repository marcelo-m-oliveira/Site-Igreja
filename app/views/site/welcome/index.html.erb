    <!-- Contact -->
    <section id="contact">
      <div class="container parallax">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading text-uppercase">Pedido de oração</h2>
            <h3 class="section-subheading text-white">Aqui você pode nos enviar o seu pedido de oração, que estaremos apresentando o seu pedido ao Senhor no nosso círculo de oração.</h3>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <form id="contactForm" name="sentMessage" novalidate="novalidate">
              <div class="row">
                <div class="col-md-6">
                  <div class="form-group">
                    <input class="form-control" id="name" type="text" placeholder="Seu nome *" required="required"
                    data-validation-required-message="Por favor nos informe o seu nome.">
                    <p class="help-block text-danger"></p>
                  </div>
                  <div class="form-group">
                    <input class="form-control" id="email" type="email" placeholder="Seu email *" required="required"
                    data-validation-required-message="Por favor informe o seu email.">
                    <p class="help-block text-danger"></p>
                  </div>
                  <div class="form-group">
                    <input class="form-control" id="phone" type="tel" placeholder="Seu telefone *" required="required"
                    data-validation-required-message="Por favor informe o seu numero.">
                    <p class="help-block text-danger"></p>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-group">
                    <textarea class="form-control" id="message" placeholder="Seu pedido de oração *" required="required"
                    data-validation-required-message="Por favor digite o seu pedido de oração."></textarea>
                    <p class="help-block text-danger"></p>
                  </div>
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-12 text-center">
                  <div id="success"></div>
                  <button id="sendMessageButton" class="btn btn-primary btn-xl text-uppercase" type="submit">Enviar pedido de oração</button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </section>